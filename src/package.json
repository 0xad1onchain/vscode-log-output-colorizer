{
    "name": "bluemix-code-log-syntax",
    "displayName": "bluemix-code-log-syntax",
    "description": "Syntax highlighting for Bluemix Code log output",
    "version": "0.0.1",
    "publisher": "AndrewTrice",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "log",
            "aliases": ["bluemix-code-log-syntax", "log"],
            "extensions": [".log"],
            "configuration": "./log.configuration.json",
            "mimetypes": ["text/x-code-output"]
        }],
        "grammars": [{
            "language": "log",
            "scopeName": "bmcode.log",
            "path": "./syntaxes/log.tmLanguage"
        }]
    }
}